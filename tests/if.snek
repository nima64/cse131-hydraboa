(let ((i 0)(j 0)(c 0))
  (loop (if (< i 3)
            (block
                (set! j 0)
                (loop
                    (if (< j 2)
                        (block 
                            (set! j (+ j 1))
                            (set! c (- c 1))
                        )
                        (break j)
                    )
                )
                (set! i (+ i 1))
            )
            (break c)
        )
    ))


